#include"control_ids.h"
Version 1 "#Version"
Form id MainForm_id at (0 0 160 160) noframe usable
Begin
 GraffitiStateIndicator AT (Right@159 Bottom@160)
 Field id LookupField_id at (0 Bottom@160 PrevLeft auto) 
  leftalign underlined singleline maxchars 32 usable editable
End
Form id SettingsForm_id at (0 0 160 160) noframe usable
Begin
 label "#dictionary" AutoID at (Center@80 0) font 1
 PushButton "Db0" id Dictionary_Pushbutton_First_id at (1 PrevBottom+1 158 auto)
 PushButton "Db1" id Dictionary_Pushbutton_First_id+1 at (1 PrevBottom+2 158 auto)
 PushButton "Db2" id Dictionary_Pushbutton_First_id+2 at (1 PrevBottom+2 158 auto)
 PushButton "Db3" id Dictionary_Pushbutton_First_id+3 at (1 PrevBottom+2 158 auto)
 PushButton "Db4" id Dictionary_Pushbutton_First_id+4 at (1 PrevBottom+2 158 auto)
 Button "#ok" id OK_Button_id at (Right@155 Bottom@158 auto 17)
 Button "#cancel" id Cancel_Button_id at (Right@(PrevLeft-10) Bottom@158 auto 17)
End

